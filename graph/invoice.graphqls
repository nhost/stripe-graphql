type Invoice {
  id: String!
  created: Int!
  currency: currencyTypes!
  customer: Customer
  hostedInvoiceUrl: String!
  livemode: Boolean!
  paid: Boolean!
  periodEnd: Int!
  periodStart: Int!
  status: String!
  subtotal: Int!
  tax: Int!
  total: Int!
  lines: Lines
}

type Lines {
  object: String
  data: [InvoiceLine!]
}

type InvoiceLine {
  id: String!
  object: String!
  amount: Int!
  currency: String!
  description: String!
  livemode: Boolean!
  quantity: Int!
  type: String!
}
