type Customer {
  id: String!
  address: Address!
  balance: Int!
  created: Int!
  currency: String!
  email: String!
  livemode: Boolean!
  name: String!
  phone: String!
  subscription: [Stripe_Subscription!] 
  invoices: [Invoice!]
}

type Address {
  city: String
  country: String
  line1: String
  line2: String
  postal_code: String
  state: String
}