type PriceRecurring {
  aggregateUsage: priceRecurringAggregateUsage!
  interval: recurringInterval!
  intervalCount: Int!
  usageType: priceRecurringUsageType!
}

type Price {
  id: String!
  object: String!
  active: Boolean!
  billingScheme: String!
  created: Int!
  currency: String!
  livemode: Boolean!
  nickname: String!
  product: Product
  recurring: PriceRecurring
}
