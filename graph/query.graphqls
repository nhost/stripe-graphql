type Query {
  customers: [Customer!]
  customer(id: String): Customer
  invoices: [Invoice!]
  invoice(id: String!): Invoice
  prices: [Price!]
  price(id: String!): Price
  paymentMethods(customer: String!, type: paymentMethodTypes!): [PaymentMethod!]
  paymentMethod(id: String!): PaymentMethod
  subscriptions: [StripeSubscription!]
  subscription(id: String!): StripeSubscription
  products: [Product!]
  product(id: String!): Product
}