type Query {
  customers: [Customer!]
  customer(id: String): Customer
  invoices: [Invoice!]
  invoice(id: String!): Invoice
  prices: [Price!]
  price(id: String!): Price
  payment_methods(customer: String!, type: payment_method_types!): [PaymentMethod!]
  payment_method(id: String!): PaymentMethod
  subscriptions: [Stripe_Subscription!]
  subscription(id: String!): Stripe_Subscription
  products: [Product!]
  product(id: String!): Product
}